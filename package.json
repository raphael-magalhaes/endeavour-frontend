{
  "name": "endeavour-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "core-js": "2.5.1",
    "material-ui": "1.0.0-beta.20",
    "material-ui-icons": "1.0.0-beta.17",
    "prop-types": "15.6.0",
    "raf": "3.4.0",
    "react": "16.1.0",
    "react-dom": "16.1.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.0.17",
    "redux": "3.7.2",
    "typeface-roboto": "0.0.43"
  },
  "devDependencies": {
    "codecov": "3.0.0",
    "eslint": "4.11.0",
    "husky": "0.14.3",
    "lint-staged": "5.0.0",
    "prettier": "1.8.0"
  },
  "scripts": {
    "report": "codecov --disable=gcov -t e208f478-c537-43c1-abac-b92013fc8065",
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill",
    "coverage": "npm run test -- --coverage",
    "YOU-SHALL-NOT-eject": "react-scripts eject"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --print-width 80 --use-tabs --no-semi --tab-width 4 --single-quote --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth ": 4,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "semi": false,
    "useTabs": true
  }
}
