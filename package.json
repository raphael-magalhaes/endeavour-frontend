{
  "name": "endeavour-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "core-js": "2.5.1",
    "material-ui": "1.0.0-beta.16",
    "material-ui-icons": "1.0.0-beta.15",
    "prop-types": "15.6.0",
    "raf": "3.4.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.0.14",
    "redux": "3.7.2",
    "typeface-roboto": "0.0.35"
  },
  "devDependencies": {
    "eslint": "4.9.0",
    "husky": "0.14.3",
    "lint-staged": "4.3.0",
    "prettier": "1.7.4"
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill",
    "YOU-SHALL-NOT-eject": "react-scripts eject"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
        "prettier --print-width 80 --use-tabs --no-semi --tab-width 4 --single-quote --write",
        "git add"
      ]
    },
    "prettier": {
      "printWidth": 80,
      "tabWidth ": 4,
      "singleQuote": true,
      "trailingComma": "none",
      "bracketSpacing": true,
      "jsxBracketSameLine": false,
      "semi": false,
      "useTabs": true
    }
}
